{
  "name": "musely-ui",
  "version": "0.1.11",
  "private": false,
  "license": "MIT",
  "author": "vencwang <fastid@gmail.com>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "lib": "node build/build-lib.js ",
    "build:entry": "node build/build-entry.js ",
    "build:theme": "gulp build --gulpfile packages/theme-default/gulpfile.js && cp-cli packages/theme-default/lib lib/theme-default",
    "build:lib": "vue-cli-service build --target lib --dest lib packages/index.ts",
    "build:pop": "babel --presets @babel/preset-typescript src -d lib",
    "build:utils": "tsc -p ./build/build-utils.json ",
    "clean": "rimraf lib && rimraf packages/*/lib",
    "dev": "yarn build:entry && vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "svg": "vsvg -s ./examples/icons/svg -t ./examples/icons/components --ext ts --es6",
    "release": "sh build/release.sh",
    "publish:docs": "node build/publish-docs.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "main": "lib/musely-ui.common.js",
  "style": "lib/theme-default/index.css",
  "typings": "types/index.d.ts",
  "files": [
    "/lib/theme-default",
    "/lib/musely-ui.common.js",
    "/types"
  ],
  "description": "vue + ts MuselyUI",
  "keyword": "vue ts typescript UI",
  "dependencies": {},
  "devDependencies": {
