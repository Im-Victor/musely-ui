{
  "compilerOptions": {
    "diagnostics": true,
    "target": "ES5",
    "module": "umd",
    "strict": true,
    "moduleResolution": "classic",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "removeComments": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": false,
    "baseUrl": "./musely-ui/lib",
    "outDir": "../lib",
    "types": ["webpack-env", "mocha", "chai"],
    "noImplicitAny": false,
    "importHelpers": false,
    "allowJs": true,
    "allowUmdGlobalAccess": true,
    "isolatedModules": true,
    "alwaysStrict": false,
    "paths": {
      "musely-ui/lib": ["musely-ui/src"]
    },
    "lib": []
  },
  "rules": {},
  "include": ["../src/mixins/**/*.ts", "../src/utils/**/*"],
  "exclude": []
}
